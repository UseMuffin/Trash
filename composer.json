{
    "name": "muffin/trash",
    "description": "Adds soft delete support to CakePHP 4 ORM tables.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "trash",
        "orm"
    ],
    "homepage": "https://github.com/usemuffin/trash",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/trash/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/trash/issues",
        "source": "https://github.com/usemuffin/trash"
    },
    "require": {
        "php": ">=7.2",
        "cakephp/orm": "4.*"
    },
    "require-dev": {
        "cakephp/chronos": "^2.0",
        "cakephp/cakephp": "4.0.*",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Trash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Trash\\Test\\": "tests"
        }
    }
}
